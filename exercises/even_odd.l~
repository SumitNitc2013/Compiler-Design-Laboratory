%{
	#include <stdio.h>
%}

number [0-9]*

%%
{number}[02468] {printf("Even\n");exit(0);}
{number}[13579] {printf("Odd\n");exit(0);}
%%

int yywrap()
{
	return 1;
}

main()
{
	//for pattern matching use lexical analyzer (generated by lex)
	//for grammar checking(sytax anlysis) use parser(generated by yacc)
	yylex();
	return 0;
}
